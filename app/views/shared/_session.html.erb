<nav class='session'>
  <ul>
    <% if current_user %>
      <li>Logged in as: <strong><%= current_user.email %></strong></li>
      <li><%= link_to 'Log out', session_path("current"), :method => :delete %></li>
    <% else %>
      <li><%= link_to 'Log in', new_session_path %></li>
      <li><%= link_to 'Sign up', new_user_path %></li>
    <% end %>
  </ul>
</nav>
